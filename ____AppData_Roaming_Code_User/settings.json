{
    "workbench.colorTheme": "Default High Contrast",
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
      {
          "before": ["leader", "j", "k"],
          "after": ["<Esc>"]
      }
    ],
    "vim.otherModesKeyBindings": [
        {
            "before": ["leader","w"],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.files.save",
                    "args": []
                }
            ]
        }
    ],
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true
}