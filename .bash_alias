alias keshi='rm -i';
alias keshigomu='rm -f';

alias mv='mv -i';
alias move='\mv';

alias cp='\cp -ip';
alias cpnew='\cp -i';
alias copy='\cp -p';
alias copynew='\cp';

alias ls='ls -ACF --show-control-chars';
alias ll='ls -l';
alias l1='ls -1';
alias lt='ls -lt';
alias ltr='ls -ltr';

alias lld='\ls -CFl | grep /';
alias l1d='\ls -CF1 | grep /';
alias ltd='\ls -CFlt | grep /';
alias ltrd='\ls -CFltr | grep /';

alias so='source';
alias sorc='source ~/.bashrc; echo "source ~/.bashrc";';

# ------------------------------------------------------------
# ▼アプリケーション
# ------------------------------------------------------------
alias ee='~/bin/ee'; # Windows Explorer
alias gvim='~/app/vim82-kaoriya-win64/gvim.exe';
alias python='winpty ~/app/Python/Python38-32/python.exe';

